#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int N, v, count = 0;
    int arr[100];

    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &v);

    for (int i = 0; i < N; i++) {
        if (arr[i] == v) {
            count++;
        }
    }

    printf("%d\n", count);
}
