#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int Total, Item, Price, Count, Compare=0;
    scanf("%d %d", &Total, &Item);
    
    for(int i=1; i <= Item; i++) {
        scanf("%d %d", &Price, &Count);
        Compare += Price * Count;
    }
    if(Total == Compare) {
        printf("Yes");
    }
    else {
        printf("No");
    }
}
