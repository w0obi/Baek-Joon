#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
int Oking=1, Oqueen=1, Orook = 2, Obishop= 2,  Oknight= 2, Opawn = 8;
int dh1, dh2, dh3, dh4, dh5, dh6;

	scanf("%d %d %d %d %d %d", &dh1, &dh2, &dh3, &dh4, &dh5, &dh6);

	printf("%d %d %d %d %d %d", Oking - dh1, Oqueen - dh2, Orook - dh3, 	Obishop - dh4, Oknight - dh5, Opawn - dh6);

return 0;
}
